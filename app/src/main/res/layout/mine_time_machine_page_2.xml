<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:background="@drawable/time_machine_page_2_bg"
    >
    <ImageView
        android:id="@+id/time_machine_page_2_word_0"
        android:layout_width="115.66dp"
        android:layout_height="65dp"
        android:layout_marginTop="46dp"
        android:layout_marginLeft="23dp"
        android:src="@drawable/time_machine_page_2_word_0"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true" />
    <ImageView
        android:id="@+id/time_machine_page_2_word_1"
        android:layout_width="145dp"
        android:layout_height="32.33dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="135dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/time_machine_page_2_word_1"
        />
    <TextView
        android:id="@+id/time_machine_page_2_movie_mount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="100dp"
        android:textSize="92px"
        android:textColor="#80fffb"
        android:text="26"/>
    <ImageView
        android:id="@+id/time_machine_page_2_word_2"
        android:layout_width="122.33dp"
        android:layout_height="42.33dp"
        android:layout_toLeftOf="@+id/time_machine_page_2_movie_mount"
        android:layout_marginRight="3dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="93dp"
        android:src="@drawable/time_machine_page_2_word_2"/>
    <ImageView
        android:id="@+id/time_machine_page_2_word_3"
        android:layout_width="122.66dp"
        android:layout_height="42.33dp"
        android:layout_toRightOf="@+id/time_machine_page_2_movie_mount"
        android:layout_marginLeft="2dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="93dp"
        android:src="@drawable/time_machine_page_2_word_3" />
    <ImageView
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:src="@drawable/down_arrow"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="13dp"
        />
</RelativeLayout>