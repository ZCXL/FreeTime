<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_gravity="center"
    android:background="@drawable/time_machine_page_1_bg"
    >
    <ImageView
        android:id="@+id/time_machine_page_1_word_0"
        android:layout_width="133.33dp"
        android:layout_height="79.33dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="23dp"
        android:src="@drawable/time_machine_page_1_word_0"
        />
    <ImageView
        android:id="@+id/time_machine_page_1_word_1"
        android:layout_width="176.66dp"
        android:layout_height="34.33dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="135dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/time_machine_page_1_word_1"
        />
    <ImageView
        android:id="@+id/time_machine_page_1_word_2"
        android:layout_width="269dp"
        android:layout_height="30dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="105dp"
        android:src="@drawable/time_machine_page_1_word_2"/>
    <ImageView
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:src="@drawable/down_arrow"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="13dp"
        />
</RelativeLayout>
